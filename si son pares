#include "stdafx.h"
#include <iostream>
#include "conio.h"
#include "math.h"

using namespace std;
void main ()
{
int num1, num2, res;
cout<< "Numero 1: ";
cin >>num1;
cout<< "Numero 2: ";
cin >>num2;
if ((num1%2)==0 &&(num2%2==0))
{
	cout<< "son pares= ";
	res=num1+num2;
}
else
    cout<< "Son Impares= ";
    res=num1*num2;
cout<<res;
getch();
}
