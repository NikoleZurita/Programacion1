#include "stdafx.h"
#include <iostream>
#include "conio.h"
#include "math.h"

using namespace std;

void main()
{
	int N,suma;
	float i;
	cout << "introducir N: ";
	cin>>N;
	suma=0;
		for(i=1;i<=N;i++)
			suma=suma+pow(i,2);
	cout << "Suma de los cuadrados de los N primeros numeros: ";
	cout  << suma;
	getch();
}
