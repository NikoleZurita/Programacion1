// serie.cpp: define el punto de entrada de la aplicaci√≥n de consola.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;
void main()
{

int f,i,N,sig;
float s,x;
cout<<"ingrese el valor de x";
cin>>x;
cout<<"ingrese el valor de N";
cin>>N;
s=0;
t=1;
sig=1;
for(i=1;<=N;i=i+2)
{
	f=f*i;
	it(i%2==1)
		s=s-pow(x,f)/f;
	else
		s=s+pow(x,i)/f;
}
cout<<s;
getch();
}
